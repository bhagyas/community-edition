<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
	
	<!-- Bootstrap Records Management Models -->   
    <bean id="org_alfresco_module_dod5015_dod5015dictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
        <property name="models">
            <list>
                <value>alfresco/module/org_alfresco_module_dod5015/model/dod5015Model.xml</value>
            </list>
        </property>
	    <property name="labels">
	            <list>
	                <value>alfresco/module/org_alfresco_module_dod5015/messages/dod5015-model</value>
	           </list>
	    </property>        
    </bean>
    
    <bean id="dod5015CustomisableTypesBootstrap" parent="customisableTypesBootstrap">    	
         <property name="customisable">
        	<list>
        		<value>dod:recordSeries</value>
        		<value>dod:recordCategory</value>
        	</list>
        </property> 
    </bean>

</beans>